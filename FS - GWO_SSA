  knn = KNeighborsClassifier(n_neighbors=k)\n","    knn.fit(xtrain, ytrain)\n","\n","    # Prediction\n","    ypred = knn.predict(xtest)\n","    acc   = np.sum(ytest == ypred) / num_test\n","\n","    total_Acc = total_Acc + acc\n","\n","  time_arr.append(time.time() - start_time)\n","\n","  Accuracy = 100 * (total_Acc/cv)\n","  print(\"Accuracy:\", Accuracy)\n","  acc_arr.append(Accuracy)\n","\n","  num_feat = fmdl['nf']\n","  print(\"Feature Size:\", num_feat)\n","  feature_size_arr.append(num_feat)\n","\n","  fitt = fmdl['c'][0].min()\n","  print(\"Fitness\", fitt)\n","  fitness.append(fitt)\n","  print(\"--------------------------------------------------\")"],"execution_count":153,"outputs":[{"output_type":"stream","name":"stdout","text":["Run  1\n","-----------------------------------\n","Iteration: 1\n","Best (SSA): 0.13342500000000002\n","Iteration: 2\n","Best (SSA): 0.13342500000000002\n","Iteration: 3\n","Best (SSA): 0.13342500000000002\n","Iteration: 4\n","Best (SSA): 0.13342500000000002\n","Iteration: 5\n","Best (SSA): 0.13342500000000002\n","Iteration: 6\n","Best (SSA): 0.13342500000000002\n","Iteration: 7\n","Best (SSA): 0.13342500000000002\n","Iteration: 8\n","Best (SSA): 0.13342500000000002\n","Iteration: 9\n","Best (SSA): 0.13342500000000002\n","Iteration: 10\n","Best (SSA): 0.13342500000000002\n","Iteration: 11\n","Best (SSA): 0.13342500000000002\n","Iteration: 12\n","Best (SSA): 0.13342500000000002\n","Iteration: 13\n","Best (SSA): 0.13342500000000002\n","Iteration: 14\n","Best (SSA): 0.13342500000000002\n","Iteration: 15\n","Best (SSA): 0.13342500000000002\n","Iteration: 16\n","Best (SSA): 0.13342500000000002\n","Iteration: 17\n","Best (SSA): 0.13342500000000002\n","Iteration: 18\n","Best (SSA): 0.13342500000000002\n","Iteration: 19\n","Best (SSA): 0.13342500000000002\n","Iteration: 20\n","Best (SSA): 0.13342500000000002\n","SF [ 0  1  4  5  6  7  8  9 14]\n","Accuracy: 87.09090909090908\n","Feature Size: 9\n","Fitness 0.13342500000000002\n","-----